{
  "expo": {
    "entryPoint": "./src/App.js",
    "name": "Tech Store",
    "description": "Tech Store e-commerce",
    "slug": "tstore",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "androidStatusBar": {
      "backgroundColor": "#FF6347"
    },
    "icon": "./src/assets/splash.png",
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FF6347",
        "foregroundImage": "./src/assets/logo.png",
        "backgroundImage": "./src/assets/icon-bg.png"
      }
    },
    "orientation": "portrait",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6347"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
